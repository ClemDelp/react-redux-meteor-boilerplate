GAAsB,uBAAlC;AACA,aAAS,MAAM,KAAf;AACD,GAHD;AAID,MAAM,kBAAkB,SAAS,cAAT,CAAwB,eAAxB,CAAxB;AACA,MAAI,eAAJ,EAAqB;AACpB,aAAS,MAAT,CAAgB,oBAAC,IAAD,IAAM,OAAO,KAAb,GAAhB,EAAwC,eAAxC;AACA;AACD,CAVD,iH","file":"/app.js","sourcesContent":["\nTemplate.body.addContent((function() {\n  var view = this;\n  return HTML.Raw('<div id=\"render-target\"></div>');\n}));\nMeteor.startup(Template.body.renderToDocument);\n","//\n// IMPORTS\n//\nimport React from 'react'\n\n//\n// COMPONENTS\n//\nlet Footer = () => {\n  return (\n   \t<b>footer</b>\n  )\n}\n\n//\n// EXPORT\n//\nexport default Footer\n","//\n// IMPORTS\n//\n\nimport React from 'react'\nimport AppBar from 'material-ui/AppBar'\n\n//\n// COMPONENT\n//\n\nlet Header = () => {\n  return (\n    <AppBar\n      title='React-redux-meteor Boilerplate'\n      iconClassNameRight='muidocs-icon-navigation-expand-more'\n    />\n\t)\n}\n\n//\n// EXPORT\n//\n\nexport default Header\n","//\n// IMPORTS\n//\nimport React from 'react'\nimport { connect } from 'react-redux'\n\n//\n// COMPONENT\n//\nconst HelloStream = ({hellos}) => {\n  return (\n    <ul>\n      {\n        hellos.map((hello, index) => {\n          return (\n            <li key={index}>{hello}</li>\n          )\n        })\n      }\n    </ul>\n  )\n}\n\n//\n// CONNECT\n//\nfunction mapStateToProps ({appReducer: {hellos}}) {\n  console.log(hellos)\n  return {hellos}\n}\nexport default connect(mapStateToProps, null)(HelloStream)\n","//\n// IMPORTS\n//\n\nimport React from 'react'\nimport { connect } from 'react-redux'\nimport Footer from '../components/Footer'\nimport Header from '../components/Header'\nimport HelloStream from './HelloStream'\nimport MuiThemeProvider from 'material-ui/styles/MuiThemeProvider'\nimport injectTapEventPlugin from 'react-tap-event-plugin'\ninjectTapEventPlugin()\n\n//\n// COMPONENT\n//\n\nconst Index = () => {\n  const styles = {\n    header: {\n      height: '10vh'\n    },\n    body: {\n      height: '85vh'\n    },\n    footer: {\n      height: '5vh'\n    }\n  }\n  return (\n    <MuiThemeProvider>\n      <div>\n        <div className='row'>\n          <div style={styles.header} className='col-xs-12 col-sm-12 col-md-12 col-lg-12'>\n            <Header />\n          </div>\n        </div>\n        <div className='row'>\n          <div style={styles.body} className='col-xs-12 col-sm-12 col-md-12 col-lg-12'>\n            <h1>BODY</h1>\n          </div>\n          <div style={styles.body} className='col-xs-12 col-sm-12 col-md-12 col-lg-12'>\n            <p>Hello Stream</p>\n            <HelloStream />\n          </div>\n        </div>\n        <div className='row'>\n          <div style={styles.footer} className='col-xs-12 col-sm-12 col-md-12 col-lg-12'>\n            <Footer />\n          </div>\n        </div>\n      </div>\n    </MuiThemeProvider>\n  )\n}\n\n//\n// EXPORT\n//\n\nexport default connect()(Index)\n","//\n// IMPORTS\n//\n\nimport React from 'react'\nimport { Provider } from 'react-redux'\nimport { Router, Route, browserHistory } from 'react-router'\nimport Index from './Index'\n\n//\n// COMPONENT\n//\n\nconst Root = ({store}) => (\n  <Provider store={store}>\n    <Router history={browserHistory}>\n      <Route path='/' component={Index} />\n      <Route path='*' component={Index} />\n    </Router>\n  </Provider>\n)\n\n//\n// EXPORT\n//\n\nexport default Root\n","//\n// IMPORTS\n//\n\nimport { combineReducers } from 'redux'\n\nexport const SAY_HELLO = 'SAY_HELLO'\n\n//\n// INITIAL STATE\n//\n\nconst intialState = {\n  hellos: []\n}\n\n//\n// REDUCER\n//\n\nfunction appReducer (state = intialState, action) {\n  switch (action.type) {\n\n    case SAY_HELLO:\n      return Object.assign({}, state, {\n        hellos: [...state.hellos, action.hello]\n      })\n\n    default:\n      return state\n  }\n}\n\n//\n// EXPORT\n//\n\nexport const rootReducer = combineReducers({\n  appReducer\n})\n\nexport function sayHello (hello) {\n  return {\n    type: SAY_HELLO,\n    hello\n  }\n}\n","// polyfills fetch on window object, if necessary\nrequire('whatwg-fetch')\n\nexport function apiRequest (endpoint, requestConfig = {}, cb) {\n  // NOTE: assumes all calls are JSON in this API\n  requestConfig.headers = {\n    'Accept': 'application/json',\n    'Content-Type': 'application/json'\n  }\n  // NOTE: assumes body wasn't already stringified\n  if (requestConfig.body) {\n    requestConfig.body = JSON.stringify(requestConfig.body)\n  }\n  return window.fetch(endpoint, requestConfig)\n  .then(res => res.json())\n  .then(json => {\n    if (cb) cb(json)\n    return json || {}\n  })\n}\n","// IMPORT\nimport React from 'react'\nimport { render } from 'react-dom'\nimport { createStore } from 'redux'\nimport { Meteor } from 'meteor/meteor'\nimport {rootReducer, sayHello} from './reducers'\nimport Root from './containers/Root'\nimport ReactDOM from 'react-dom'\nimport {apiRequest} from './utils/api'\n\n// CREATE REDUX SOTRE\nconst store = createStore(\n  rootReducer, {}, window.devToolsExtension ? window.devToolsExtension() : undefined\n)\n\n// APP\nMeteor.startup(() => {\n  apiRequest('/hello')\n  Streamy.on('hello', (hello) => {\n    console.log('received ' + hello + ' from hello stream...')\n    sayHello(hello.hello)\n  })\n\tconst reactDivElement = document.getElementById('render-target')\n\tif (reactDivElement) {\n\t\tReactDOM.render(<Root store={store} />, reactDivElement)\n\t}\n})\n"]}}]