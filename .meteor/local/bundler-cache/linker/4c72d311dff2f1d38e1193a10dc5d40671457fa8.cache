,WAAqB;AAAA,MAAR,MAAQ;;AACzD,UAAQ,OAAO,IAAf;;AAEE,SAAK,SAAL;AACE,aAAO,OAAO,MAAP,CAAc,EAAd,EAAkB,KAAlB,EAAyB;AAC9B,6CAAY,MAAM,MAAlB,IAA0B,OAAO,KAAjC;AAD8B,OAAzB,CAAP;;AAIF,SAAK,SAAL;AACE,aAAO,OAAO,MAAP,CAAc,EAAd,EAAkB,KAAlB,EAAyB;AAC9B,4CAAW,MAAM,KAAjB,IAAwB,OAAO,CAA/B;AAD8B,OAAzB,CAAP;;AAIF;AACE,aAAO,KAAP;AAbJ;AAeD;;;;;;AAMD,OAAO,SAAS,QAAT,CAAmB,KAAnB,EAA0B;AAC/B,SAAO;AACL,UAAM,SADD;AAEL;AAFK,GAAP;AAID;;AAED,OAAO,SAAS,QAAT,CAAmB,CAAnB,EAAsB;AAC3B,SAAO;AACL,UAAM,SADD;AAEL;AAFK,GAAP;AAID,iH;;;;;;;;;;;;;;;AClDD,SAAS,eAAT,QAAgC,OAAhC;AACA,OAAO,IAAP,MAAiB,QAAjB;;;;;;AAMA,IAAM,cAAc,gBAAgB;AAClC;AADkC,CAAhB,CAApB;;;;;AAOA,eAAe,WAAf,uF;;;;;;;;;;;;;eCXW,a,EAIA,uB,EAIe,Q;;AAf1B,SAAS,IAAT,EAAe,IAAf,EAAqB,GAArB,EAA0B,IAA1B,EAAgC,MAAhC,EAAwC,IAAxC,QAAoD,oBAApD;AACA,SAAS,SAAT,QAA0B,YAA1B;;AAEA,SACE,SADF,QAEO,kBAFP;;AAIA,SAAW,aAAX,CAA0B,MAA1B;AAAA;AAAA;AAAA;AAAA;AAAA;AACE,oBAAQ,GAAR,CAAY,wBAAZ,EAAsC,OAAO,KAA7C;;AADF;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAIA,SAAW,uBAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,2CACU,UAAU,SAAV,EAAqB,aAArB,CADV;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAIA,eAAe,SAAW,QAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mBACP,CACJ,KAAK,uBAAL,CADI,CADO;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA,iH;;;;;;;;;;;;ACdf,QAAQ,cAAR;;AAEA,OAAO,SAAS,UAAT,CAAqB,QAArB,EAAuD;AAAA,MAAxB,aAAwB,yDAAR,EAAQ;AAAA,MAAJ,EAAI;;;AAE5D,gBAAc,OAAd,GAAwB;AACtB,cAAU,kBADY;AAEtB,oBAAgB;AAFM,GAAxB;;AAKA,MAAI,cAAc,IAAlB,EAAwB;AACtB,kBAAc,IAAd,GAAqB,KAAK,SAAL,CAAe,cAAc,IAA7B,CAArB;AACD;AACD,SAAO,OAAO,KAAP,CAAa,QAAb,EAAuB,aAAvB,EACN,IADM,CACD;AAAA,WAAO,IAAI,IAAJ,EAAP;AAAA,GADC,EAEN,IAFM,CAED,gBAAQ;AACZ,QAAI,EAAJ,EAAQ,GAAG,IAAH;AACR,WAAO,QAAQ,EAAf;AACD,GALM,CAAP;AAMD,iH;;;;;;;;;;;;AClBD,OAAO,gBAAP;AACA,OAAO,KAAP,MAAkB,OAAlB;AACA,SAAS,MAAT,QAAuB,WAAvB;AACA,SAAS,eAAT,EAA0B,OAA1B,EAAmC,WAAnC,QAAsD,OAAtD;AACA,SAAS,MAAT,QAAuB,eAAvB;AACA,OAAO,WAAP,MAAwB,YAAxB;AACA,OAAO,IAAP,MAAiB,mBAAjB;AACA,OAAO,QAAP,MAAqB,WAArB;AACA,SAAQ,QAAR,QAAuB,iBAAvB;;;;;AAKA,OAAO,QAAP,MAAqB,SAArB;AACA,OAAO,oBAAP,MAAiC,YAAjC;AACA,IAAM,iBAAiB,sBAAvB;;;;;AAKA,IAAM,QAAQ,YACZ,WADY,EAEZ,QACE,gBAAgB,cAAhB,CADF,EAEE,OAAO,iBAAP,GACE,OAAO,iBAAP,EADF,GAEE;AAAA,SAAK,CAAL;AAAA,CAJJ,CAFY,CAAd;AASA,eAAe,GAAf,CAAmB,QAAnB;;;;AAIA,OAAO,OAAP,CAAe,YAAM;;AAEnB,UAAQ,EAAR,CAAW,OAAX,EAAoB,UAAU,IAAV,EAAgB;AAClC,UAAM,QAAN,CAAe,SAAS,KAAK,IAAd,CAAf;AACD,GAFD;AAGA,UAAQ,EAAR,CAAW,aAAX,EAA0B,UAAU,CAAV,EAAa;AACrC,UAAM,QAAN,CAAe,SAAS,CAAT,CAAf;AACD,GAFD;;;AAKD,MAAM,kBAAkB,SAAS,cAAT,CAAwB,eAAxB,CAAxB;AACA,MAAI,eAAJ,EAAqB;AACpB,aAAS,MAAT,CAAgB,oBAAC,IAAD,IAAM,OAAO,KAAb,GAAhB,EAAwC,eAAxC;AACA;AACD,CAdD;;;AAiBA,OAAO,MAAP,GAAiB,UAAU,kBAAV,EAA8B;AAC7C,SAAO,UAAU,KAAV,EAAiB,IAAjB,EAAuB;AAC5B,QAAI,EAAE,QAAQ,EAAE,GAAF,CAAM,IAAN,EAAY,KAAZ,CAAV,CAAJ,EACE,mBAAmB,KAAnB,EAA0B,IAA1B;AACH,GAHD;AAID,CALe,CAKb,OAAO,MALM,CAAhB,iG","file":"/app.js","sourcesContent":["\nTemplate.body.addContent((function() {\n  var view = this;\n  return HTML.Raw('<div id=\"render-target\"></div>');\n}));\nMeteor.startup(Template.body.renderToDocument);\n","//\n// IMPORTS\n//\nimport React from 'react'\n\n//\n// COMPONENTS\n//\nlet Footer = () => {\n  return (\n   \t<b>FOOTER</b>\n  )\n}\n\n//\n// EXPORT\n//\nexport default Footer\n","//\n// IMPORTS\n//\n\nimport React from 'react'\nimport AppBar from 'material-ui/AppBar'\n\n//\n// COMPONENT\n//\n\nlet Header = () => {\n  return (\n  \t<AppBar\n      title='Meteor react-redux boilerplate'\n      iconClassNameRight='muidocs-icon-navigation-expand-more'\n    />\n\t)\n}\n\n//\n// EXPORT\n//\n\nexport default Header\n","//\n// IMPORTS\n//\n\nimport React from 'react'\nimport { connect } from 'react-redux'\nimport Footer from '../components/Footer'\nimport Header from '../components/Header'\nimport MuiThemeProvider from 'material-ui/styles/MuiThemeProvider'\nimport injectTapEventPlugin from 'react-tap-event-plugin'\ninjectTapEventPlugin()\n\nimport {apiRequest} from '../utils/api'\n\n// To remove\nimport {demo} from '../demo/'\n\n//\n// COMPONENT\n//\n\nconst Index = () => {\n  const styles = {\n    header: {\n      height: '10vh'\n    },\n    body: {\n      height: '85vh'\n    },\n    footer: {\n      height: '5vh'\n    }\n  }\n  return (\n    <MuiThemeProvider>\n      <div>\n        <div className='row'>\n          <div style={styles.header} className='col-xs-12 col-sm-12 col-md-12 col-lg-12'>\n            <Header />\n          </div>\n        </div>\n        <div className='row'>\n          <Demo />\n        </div>\n        <div className='row'>\n          <div style={styles.footer} className='col-xs-12 col-sm-12 col-md-12 col-lg-12'>\n            <Footer />\n          </div>\n        </div>\n      </div>\n    </MuiThemeProvider>\n  )\n}\n\n//\n// CONNECT\n//\n\nfunction mapStateToProps ({data: {hellos}}) {\n  return {hellos}\n}\n\nexport default connect(\n  mapStateToProps,\n  null\n)(Index)\n","//\n// IMPORTS\n//\n\nimport React from 'react'\nimport { Provider } from 'react-redux'\nimport { Router, Route, browserHistory } from 'react-router'\n\nimport Index from './Index'\n\n//\n// COMPONENT\n//\n\nconst Root = ({store}) => (\n  <Provider store={store}>\n    <Router history={browserHistory}>\n      <Route path='/' component={Index} />\n      <Route path='*' component={Index} />\n    </Router>\n  </Provider>\n)\n\n//\n// EXPORT\n//\n\nexport default Root\n","//\n// IMPORTS\n//\nimport React from 'react'\nimport { connect } from 'react-redux'\n\n//\n// COMPONENT\n//\nconst HelloStream = ({hellos}) => {\n  return (\n    <ul>\n      {\n        hellos.map((hello, index) => {\n          return (\n            <li key={index}>{hello}</li>\n          )\n        })\n      }\n    </ul>\n  )\n}\n\n//\n// CONNECT\n//\n\nfunction mapStateToProps ({data: {hellos}}) {\n  return {hellos}\n}\n\nexport default connect(mapStateToProps, null)(HelloStream)\n","// MODULE\n\nimport React from 'react'\n\n// COMPONENT\nexport default class PlotlyChart extends React.Component {\n  constructor (props) {\n    super(props)\n  }\n\n  // first time\n  componentDidMount () {\n    // const x = this.range(0, this.props.y.length)\n    const y = this.props.y\n    var trace = {\n      name: 'data',\n      x: this.props.x,\n      y: y,\n      type: 'scatter',\n      opacity: 0.75,\n      marker: {color: this.props.color}\n    }\n    var trace_2 = {\n      name: 'avg',\n      x: this.props.x.filter((el)=>{\n        return (el % this.props.average === 0)\n      }),\n      y: this.props.averageStream,\n      type: 'scatter',\n      opacity: 1,\n      marker: {color: 'red'}\n    }\n    var data = [trace,trace_2]\n    var layout = {\n      title: this.props.name,\n      showlegend: true,\n      xaxis: {\n        autorange: true\n      },\n      yaxis: {\n        autorange: true\n      }\n    }\n    if (this.props.type) {\n      layout.xaxis.type = this.props.type\n      layout.yaxis.type = this.props.type\n    }\n    Plotly.newPlot(this.refs.graphViz, data, layout)\n  }\n\n  range (start, end) {\n    var foo = []\n    for (var i = start; i < end; i++) {\n      foo.push(i)\n    }\n    return foo\n  }\n  // after first time\n  componentDidUpdate () {\n    // const trace = this.getTrace()\n    const index = this.props.y.length\n    const x = this.props.x[index-1]\n    const y = this.props.y[index-1]\n    this.refs.graphViz.data[0].x.push(x)\n    this.refs.graphViz.data[0].y.push(y)\n    // average\n    this.refs.graphViz.data[1].x = this.props.x.filter((el)=>{\n      return (el%this.props.average == 0)\n    })\n    this.refs.graphViz.data[1].y = this.props.averageStream\n    Plotly.redraw(this.refs.graphViz)\n  }\n  // Does'nt call the render when state change\n  componentWillReceiveProps (nextProps, nextContext) {\n\n  }\n\n  render () {\n    return (\n      <div ref='graphViz'></div>\n    )\n  }\n}\n","//\n// IMPORTS\n//\n\nimport React from 'react'\nimport { connect } from 'react-redux'\nimport HelloStream from './HelloStream'\nimport PlotlyChart from './Plotly'\n\n//\n// COMPONENT\n//\n\nconst Demo = () => {\n  return (\n      <div>\n        <div className='row'>\n          <div className='col-xs-6 col-sm-6 col-md-6 col-lg-6'>\n            <p>Hello Stream</p>\n            <button onClick={() => {\n              // test api route and stream connection\n              apiRequest('/hello')\n            }}>Test the stream</button>\n            <HelloStream />\n          </div>\n          <div className='col-xs-6 col-sm-6 col-md-6 col-lg-6'>\n            <PlotlyChart\n              x={x}\n              y={score}\n              name='Score'\n              type='log'\n              color='#0000ff'\n              average={average}\n              averageStream={averageStream}\n            />\n          </div>\n        </div>\n      </div>\n    )\n}\n\n//\n// CONNECT\n//\n\nfunction mapStateToProps ({demo: {hellos, score}}) {\n  const x = [0]\n  if (score.length > 1) {\n    for (let i = 1; i < score.length; i++) {\n      x.push(i)\n    }\n  }\n  return {hellos, score, x}\n}\n\nexport default connect(\n  mapStateToProps,\n  null\n)(Demo)\n","//\n// IMPORTS\n//\n\nexport const SAY_HELLO = 'SAY_HELLO'\nexport const ADD_SCORE = 'ADD_SCORE'\n\n//\n// INITIAL STATE\n//\n\nconst intialState = {\n  hellos: [],\n  score: []\n}\n\n//\n// REDUCER\n//\n\nexport default function demo (state = intialState, action) {\n  switch (action.type) {\n\n    case SAY_HELLO:\n      return Object.assign({}, state, {\n        hellos: [...state.hellos, action.hello]\n      })\n\n    case ADD_SCORE:\n      return Object.assign({}, state, {\n        score: [...state.score, action.y]\n      })\n\n    default:\n      return state\n  }\n}\n\n//\n// EXPORT\n//\n\nexport function sayHello (hello) {\n  return {\n    type: SAY_HELLO,\n    hello\n  }\n}\n\nexport function addScore (y) {\n  return {\n    type: ADD_SCORE,\n    y\n  }\n}\n","//\n// IMPORTS\n//\n\nimport { combineReducers } from 'redux'\nimport demo from './demo'\n\n//\n// COMBINE\n//\n\nconst rootReducer = combineReducers({\n  demo\n})\n\n//\n// EXPORT\n//\nexport default rootReducer\n","import { call, fork, put, race, select, take } from 'redux-saga/effects'\nimport { takeEvery } from 'redux-saga'\n\nimport {\n  SAY_HELLO\n} from '../reducers/data'\n\nfunction * sayHelloSagas (action) {\n  console.log('say hello from sagas: ', action.hello)\n}\n\nfunction * watchSayHelloFromStream () {\n  yield * takeEvery(SAY_HELLO, sayHelloSagas)\n}\n\nexport default function * rootSaga () {\n  yield [\n    fork(watchSayHelloFromStream)\n  ]\n}\n","// polyfills fetch on window object, if necessary\nrequire('whatwg-fetch')\n\nexport function apiRequest (endpoint, requestConfig = {}, cb) {\n  // NOTE: assumes all calls are JSON in this API\n  requestConfig.headers = {\n    'Accept': 'application/json',\n    'Content-Type': 'application/json'\n  }\n  // NOTE: assumes body wasn't already stringified\n  if (requestConfig.body) {\n    requestConfig.body = JSON.stringify(requestConfig.body)\n  }\n  return window.fetch(endpoint, requestConfig)\n  .then(res => res.json())\n  .then(json => {\n    if (cb) cb(json)\n    return json || {}\n  })\n}\n","// IMPORT\nimport 'babel-polyfill'\nimport React from 'react'\nimport { render } from 'react-dom'\nimport { applyMiddleware, compose, createStore } from 'redux'\nimport { Meteor } from 'meteor/meteor'\nimport rootReducer from './reducers'\nimport Root from './containers/Root'\nimport ReactDOM from 'react-dom'\nimport {sayHello} from './reducers/data'\n\n//\n// SAGA\n//\nimport rootSaga from './sagas'\nimport createSagaMiddleware from 'redux-saga'\nconst sagaMiddleware = createSagaMiddleware()\n\n//\n// STORE\n//\nconst store = createStore(\n  rootReducer,\n  compose(\n    applyMiddleware(sagaMiddleware),\n    window.devToolsExtension\n    ? window.devToolsExtension()\n    : f => f\n  )\n)\nsagaMiddleware.run(rootSaga)\n\n// APP\n\nMeteor.startup(() => {\n  // STREAMS\n  Streamy.on('hello', function (json) {\n    store.dispatch(sayHello(json.data))\n  })\n  Streamy.on('scoreStream', function (y) {\n    store.dispatch(addScore(y))\n  })\n\n  // mount app\n\tconst reactDivElement = document.getElementById('render-target')\n\tif (reactDivElement) {\n\t\tReactDOM.render(<Root store={store} />, reactDivElement)\n\t}\n})\n\n// Since we don't want all those debug messages\nMeteor._debug = (function (super_meteor_debug) {\n  return function (error, info) {\n    if (!(info && _.has(info, 'msg')))\n      super_meteor_debug(error, info)\n  }\n})(Meteor._debug);\n"]}}]